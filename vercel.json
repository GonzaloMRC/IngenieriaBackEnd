{
    "version":2,

    "functions": [
        {
            "queryapp/*.js": {
                "memory": 3008,
                "maxDuration": 30
              }
        }
        
    ],

    "builds": [
        {
            "src": "igenieriabackend/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9" 
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "igenieriabackend/wsgi.py"
        }
    ]

    
}