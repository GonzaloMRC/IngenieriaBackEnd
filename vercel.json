{
    "functions": {
      "api/*.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "function": "igenieriabackend/wsgi.application"
      }
    ]
}
  
  
  
  