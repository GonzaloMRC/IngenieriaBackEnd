{
    "functions": {
        "api/*.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "igenieriabackend/wsgi.application"
        }
    ]
}
  